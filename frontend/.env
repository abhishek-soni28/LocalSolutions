# API Configuration
REACT_APP_API_URL=http://localhost:8080/api
REACT_APP_WS_URL=ws://localhost:8080/ws

# Authentication Configuration
REACT_APP_JWT_EXPIRATION=86400000
REACT_APP_REFRESH_TOKEN_EXPIRATION=604800000

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Stripe Configuration
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Analytics Configuration
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PAYMENTS=true
REACT_APP_ENABLE_MAPS=true

# Performance Configuration
REACT_APP_DEBOUNCE_DELAY=500
REACT_APP_INFINITE_SCROLL_THRESHOLD=100
REACT_APP_CACHE_EXPIRATION=3600000

# UI Configuration
REACT_APP_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_DATE_FORMAT=MM/DD/YYYY
REACT_APP_TIME_FORMAT=HH:mm:ss
REACT_APP_CURRENCY=USD

# Development Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_REDUX_DEVTOOLS=true